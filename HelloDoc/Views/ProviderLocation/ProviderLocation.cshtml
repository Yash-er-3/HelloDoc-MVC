@{
    Layout = "header_dashboard";
}
<div class="container pb-4 px-0 px-lg-5" id="providerLocationMainDiv">
    <div class="d-flex justify-content-between align-items-center w-100 my-3">
        <h4 class="fw-bold">Provider Location</h4>
        <a asp-controller="Admin" asp-action="AdminDashboard"
           class="btn m-0 btn-outline-info  px-2 ">
            <i class="fa-solid fa-chevron-left"></i>
            Back
        </a>
    </div>

    <div class="container-fluid shadow bg-white">
        <div class="row mt-2 p-3">
            <div id="map"></div>
        </div>
    </div>

</div>
<script>
    window.onload = function () {
        $('.admin-layout-nav').removeClass('admin-layout-nav-active');
        $('#nav-provider-location-tab').addClass('admin-layout-nav-active');
    }
</script>
<style>
    #map {
        height: 70vh;
    }
</style>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>

<script src="/js/ProviderLocation/ProviderLoaction.js" asp-append-version="true"></script>