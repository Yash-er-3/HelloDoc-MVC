@model Services.Viewmodels.PatientHistoryViewModel@{    Layout = "header_dashboard";}<div class="container-fluid pb-4 px-0 px-lg-5 mt-5" id="recordsmaindiv">    <div class="my-3 d-flex justify-content-between">        <h4 class="fw-bold">Patient History</h4>    </div>    <div class="container-fluid shadow px-3 py-4 bg-white">        <div class="row d-flex">            <div class="col-md-3">                <div class="form-floating">                    <input id="firstNamePatientHistory" class="form-control rounded-1  shadow-none" type="search" placeholder="Search..." aria-label="Search" style="width:clamp(100px, 100%, 100%);">                    <label class="FloatingInput">First Name</label>                </div>            </div>            <div class="col-md-3">                <div class="form-floating">                    <input id="lastNamePatientHistory" class="form-control rounded-1 shadow-none" type="search" placeholder="Provider Name" style="width:clamp(100px, 100%, 100%);">                    <label class="FloatingInput">Last Name</label>                </div>            </div>            <div class="col-md-3">                <div class="form-floating">                    <input id="emailPatientHistory" class="form-control rounded-1 shadow-none" type="text" placeholder="Email" style="width:clamp(100px, 100%, 100%);">                    <label class="FloatingInput">Email</label>                </div>            </div>            <div class="col-md-3">                <div class="form-floating">                    <input id="phonePatientHistory" class="form-control rounded-1 shadow-none" type="text" placeholder="Phone Number" style="width:clamp(100px, 100%, 100%);">                    <label class="FloatingInput">Phone Number</label>                </div>            </div>            <div class="d-flex justify-content-end mt-3">                <button class="btn btn-outline-info rounded-3 py-2 px-3 mx-2 shadow-none" id="clearPatientHistorybtn">                    Clear                </button>                <button class="btn-info shadow-none rounded-3" id="searchPatientHistorybtn">                    Search                </button>            </div>        </div>    <div id="patientHistoryTable" class="table-responsive pt-3">    </div>    </div></div>@* <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>*@<script src="/js/Records/PatientHistory.js" asp-append-version="true"></script>