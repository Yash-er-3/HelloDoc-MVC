    <link rel="stylesheet" href="~/css/submitrequest.css" />
    <link rel="stylesheet" href="~/css/admin_dashboard.css" asp-append-version="true" />
</head>

<body>


    <nav class="navbar navbar-light bg-light rounded px-4">
        <a asp-action="Admin" asp-controller="Admin" height="25" width="25">
            <img class="hallodoc-img-1" src="~/images/hallodoc.png" alt="">
        </a>
        <div class="navdiv d-flex flex-row gap-2">

            @{
                try
                {
                    int id = (int)Context.Session.GetInt32("AdminId");
                    String username = Context.Session.GetString("UserName");

                    if (username != null)
                    {
                        <h6 class="mt-2 text-secondary d-sm-block d-none">Welcome , <strong>@username</strong></h6>

                        <a asp-action="registeredpatient" asp-controller="Home">
                            <button style="background-color: transparent;" class="border-1 rounded text-info border-info p-2">
                                <a asp-action="Logout" asp-controller="CredentialAdmin" class="text-decoration-none">
                                    <div class="logouttext"><span class="">Log out</span></div>
                                </a>
                            </button>
                        </a>
                        //<div class="logoutimg"><i class="fa-solid fa-arrow-right-from-bracket "></i></div>
                    }
                }
                catch { }
            }
            <div id="img-btn">
                <img class="light-mode" role="button" src="~/images/light.png" alt="" id="img-mode">
            </div>
        </div>
    </nav>



    <div class="mainnav bg-white">
        <nav class="shadow">
            <div class="ps-4 nav nav-tabs border-bottom-0 border-start-0 border-end-0 border border-2" id="nav-tab" role="tablist">
                <button class="nav-link active me-4" id="nav-home-tab" data-bs-toggle="tab" data-bs-target="#nav-home" type="button" role="tab" aria-controls="nav-home" aria-selected="true">Dashboard</button>
                <button class="nav-link me-4" id="nav-provider-location-tab" data-bs-toggle="tab" data-bs-target="#nav-provider-location" type="button" role="tab" aria-controls="nav-provider-location" aria-selected="false">Provider Location</button>
                <button class="nav-link me-4" id="nav-profile-tab" data-bs-toggle="tab" data-bs-target="#nav-profile" type="button" role="tab" aria-controls="nav-profile" aria-selected="false">My Profile</button>
                <button class="nav-link me-4" id="nav-provider-tab" data-bs-toggle="tab" data-bs-target="#nav-provider" type="button" role="tab" aria-controls="nav-provider" aria-selected="false">Providers</button>
                <button class="nav-link me-4" id="nav-partner-tab" data-bs-toggle="tab" data-bs-target="#nav-partner" type="button" role="tab" aria-controls="nav-partner" aria-selected="false">Partners</button>
                <button class="nav-link me-4" id="nav-access-tab" data-bs-toggle="tab" data-bs-target="#nav-access" type="button" role="tab" aria-controls="nav-access" aria-selected="false">Access</button>
                <button class="nav-link me-4" id="nav-record-tab" data-bs-toggle="tab" data-bs-target="#nav-record" type="button" role="tab" aria-controls="nav-record" aria-selected="false">Records</button>
            </div>
        </nav>
    </div>

    @RenderBody()


