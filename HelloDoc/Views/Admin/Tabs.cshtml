  break;                case '#s_toclose':                    url = '/YourController/ProviderLocationAction';                    break;                case '#s_unpaid':                    url = '/YourController/ProviderLocationAction';                    break;                default:                    url = '../Home/AdminLogin';            }            $.ajax({                url: url,                success: function (response) {                    $('#status-tabContent').html(response);                },                error: function (xhr, status, error) {                    console.error(error);                }            });        });    });</script>